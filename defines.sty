	\vspace{1mm}
}

\newcommand{\define}[1]{\hd{Определение #1}}

\newcommand{\theorem}{\hd{Теорема}}

\newcommand{\proof}{\hd{Доказательство}}

\newcommand{\onlyif}{\hd{Необходимость}}

\newcommand{\enough}{\hd{Достаточность}}

